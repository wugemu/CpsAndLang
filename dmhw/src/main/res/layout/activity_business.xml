<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:background="#f2f2f2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.AboutActivity340">

    <View
        android:id="@+id/bar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_24"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_8"
            android:layout_centerVertical="true">

            <ImageView
                android:layout_width="@dimen/dp_9"
                android:layout_height="@dimen/dp_18"
                android:layout_margin="@dimen/dp_9"
                android:scaleType="centerCrop"
                android:src="@mipmap/btn_back_default" />

        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_marginBottom="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_10"
            android:text="商务合作"
            android:textColor="#333333"
            android:textSize="@dimen/sp_18"
            tools:ignore="HardcodedText" />

    </RelativeLayout>
    <ImageView
        android:layout_width="@dimen/dp_136"
        android:layout_height="@dimen/dp_43"
        android:scaleType="fitXY"
        android:layout_marginTop="@dimen/dp_23"
        android:layout_gravity="center_horizontal"
        android:src="@mipmap/bussiness_logo"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none"
        android:layout_marginBottom="@dimen/sp_13"
        >
         <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:focusable="true"
        android:focusableInTouchMode="true"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center_horizontal"
        android:layout_marginTop="@dimen/dp_38"
        android:background="@drawable/personal_grid_bg"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_10"
        android:paddingTop="@dimen/dp_10"
        android:paddingBottom="@dimen/dp_10"
        android:paddingStart="@dimen/dp_12"
        android:paddingEnd="@dimen/dp_12"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="*"
                android:textColor="#EE2532"
                android:textSize="@dimen/sp_15"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="品牌/公司"
                android:textColor="#333333"
                android:textSize="@dimen/sp_15"
                android:layout_marginStart="@dimen/dp_5"
                tools:ignore="HardcodedText" />

            <EditText
                android:id="@+id/company_name"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="right|center"
                android:background="@null"
                android:hint="请输入公司名称"
                android:lines="1"
                android:singleLine="true"
                android:textColor="#666666"
                android:textColorHint="#ACACAC"
                android:textSize="@dimen/sp_14"
                tools:ignore="HardcodedText"
                android:textCursorDrawable="@drawable/my_cursor"
                android:cursorVisible="true" />


        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_0_5"
            android:background="#EEEEEE"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="*"
                android:textColor="#EE2532"
                android:textSize="@dimen/sp_15"
                android:visibility="invisible"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="您的岗位"
                android:textColor="#333333"
                android:textSize="@dimen/sp_15"
                android:layout_marginStart="@dimen/dp_5"
                tools:ignore="HardcodedText" />

            <EditText
                android:id="@+id/job_name"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="right|center"
                android:background="@null"
                android:hint="请输入您的岗位"
                android:lines="1"
                android:singleLine="true"
                android:textColor="#666666"
                android:textColorHint="#ACACAC"
                android:textSize="@dimen/sp_14"
                tools:ignore="HardcodedText"
                android:textCursorDrawable="@drawable/my_cursor"
                android:cursorVisible="true" />


        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_0_5"
            android:background="#EEEEEE"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="*"
                android:textColor="#EE2532"
                android:textSize="@dimen/sp_15"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="您的姓名"
                android:textColor="#333333"
                android:textSize="@dimen/sp_15"
                android:layout_marginStart="@dimen/dp_5"
                tools:ignore="HardcodedText" />

            <EditText
                android:id="@+id/your_name"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="right|center"
                android:background="@null"
                android:hint="请输入您的姓名"
                android:lines="1"
                android:singleLine="true"
                android:textColor="#666666"
                android:textColorHint="#ACACAC"
                android:textSize="@dimen/sp_14"
                tools:ignore="HardcodedText"
                android:textCursorDrawable="@drawable/my_cursor"
                android:cursorVisible="true" />


        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_0_5"
            android:background="#EEEEEE"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="*"
                android:textColor="#EE2532"
                android:textSize="@dimen/sp_15"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="联系电话"
                android:textColor="#333333"
                android:textSize="@dimen/sp_15"
                android:layout_marginStart="@dimen/dp_5"
                tools:ignore="HardcodedText" />

            <EditText
                android:id="@+id/your_phone"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="right|center"
                android:background="@null"
                android:hint="请输入您的电话"
                android:lines="1"
                android:singleLine="true"
                android:textColor="#666666"
                android:textColorHint="#ACACAC"
                android:textSize="@dimen/sp_14"
                tools:ignore="HardcodedText"
                android:textCursorDrawable="@drawable/my_cursor"
                android:cursorVisible="true"/>


        </LinearLayout>


    </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center_horizontal"
            android:layout_marginTop="@dimen/dp_12"
            android:background="@drawable/business_bg"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_marginEnd="@dimen/dp_10"
            android:paddingTop="@dimen/dp_10"
            android:paddingBottom="@dimen/dp_15"
            android:paddingStart="@dimen/dp_12"
            android:paddingEnd="@dimen/dp_12"
            >
            <LinearLayout
                android:id="@+id/business_type_layout"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="*"
                    android:textColor="#EE2532"
                    android:textSize="@dimen/sp_15"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_15"
                    android:layout_marginStart="@dimen/dp_5"
                    android:text="意向合作"
                    tools:ignore="HardcodedText" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="right"
                    tools:ignore="RtlHardcoded">

                    <TextView
                        android:id="@+id/business_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_13"
                        android:textColor="#666666"
                        android:layout_marginRight="@dimen/dp_8"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@+id/arr_02"
                        android:text="团队入驻"
                        tools:ignore="HardcodedText" />

                    <ImageView
                        android:id="@+id/arr_02"
                        android:layout_width="@dimen/dp_7"
                        android:layout_height="@dimen/dp_13"
                        android:layout_marginStart="@dimen/dp_4"
                        android:layout_marginTop="@dimen/dp_2"
                        android:layout_centerVertical="true"
                        android:src="@mipmap/arrow"/>


                </RelativeLayout>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_0_5"
                android:background="#EEEEEE"
                android:layout_marginBottom="@dimen/dp_10"
                />

            <TextView
                android:id="@+id/business_explain"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#ACACAC"
                android:textSize="@dimen/sp_13"
                android:text="@string/str_bussiness_team"
                />


        </LinearLayout>
    </LinearLayout>
      </ScrollView>
        <LinearLayout
            android:id="@+id/business_sumit_btn"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45"
            android:background="@drawable/nocheck_btn_bg"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_marginBottom="@dimen/dp_10"
            android:gravity="center">

            <TextView
                android:id="@+id/business_sumit_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_15"
                android:textColor="#999999"
                android:text="提交"
                tools:ignore="HardcodedText" />

        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
