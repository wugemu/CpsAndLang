<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/total_layout"
    >

    <LinearLayout
        android:id="@+id/adapter_new_one_fragment_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_10"
        android:orientation="horizontal">

        <RelativeLayout
            android:layout_width="@dimen/dp_96"
            android:layout_height="@dimen/dp_96">

            <ImageView
                android:id="@+id/adapter_new_one_fragment_image"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                android:background="@mipmap/default_for_pic"/>

            <ImageView
                android:id="@+id/adapter_new_one_fragment_video"
                android:layout_width="@dimen/dp_59"
                android:layout_height="@dimen/dp_59"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:visibility="gone"
                android:scaleType="centerCrop"
                android:src="@mipmap/commodity_video"/>

            <LinearLayout
                android:id="@+id/oneframent_room_layout"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_17"
                android:orientation="horizontal"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="@dimen/dp_5"
                android:layout_marginStart="@dimen/dp_5"
                android:layout_marginEnd="@dimen/dp_5"
                android:visibility="gone"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="bottom"
                    android:orientation="horizontal"
                    android:background="@mipmap/free_bg"
                    >

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:textColor="#ffffff"
                        android:layout_gravity="center_vertical"
                        android:textSize="@dimen/sp_11"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/dp_3"
                        android:paddingRight="@dimen/dp_3"
                        android:paddingBottom="@dimen/dp_2"
                        android:paddingTop="@dimen/dp_2"
                        />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_weight="2"
                        android:layout_gravity="center_vertical"
                        android:paddingLeft="@dimen/dp_3"
                        android:paddingRight="@dimen/dp_3"
                        android:paddingBottom="@dimen/dp_2"
                        android:paddingTop="@dimen/dp_2"
                        >
                    <TextView
                        android:id="@+id/adapter_new_room_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_5"
                        android:includeFontPadding="false"
                        android:gravity="center_vertical"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/sp_11"
                        tools:text="298" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="观看"
                        android:gravity="center_vertical"
                        android:includeFontPadding="false"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/sp_11"
                         />
                    </LinearLayout>
                </LinearLayout>


            </LinearLayout>

        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_2"
            android:layout_marginBottom="@dimen/dp_2"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/adapter_new_one_fragment_check"
                    android:layout_width="@dimen/dp_13"
                    android:layout_height="@dimen/dp_13"
                    android:src="@mipmap/taobao_icon"
                    android:scaleType="fitXY"
                    android:layout_gravity="center_vertical"
                    />
                <TextView
                    android:id="@+id/adapter_new_one_fragment_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/sp_13"
                    android:textColor="#333333"
                    android:layout_marginStart="@dimen/dp_3"
                    android:layout_gravity="center_vertical"
                    android:lines="1"
                    android:ellipsize="end"
                    tools:text="测试测试测试测试测试测试测试测试测试测试"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginTop="@dimen/dp_3"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/adapter_new_one_fragment_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/sp_12"
                    android:textColor="#999999"
                    android:layout_gravity="center_vertical"
                    tools:text="1000"/>
                <TextView
                    android:id="@+id/adapter_collection_shop"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:includeFontPadding="false"
                    android:lineSpacingMultiplier="0.9"
                    android:lines="1"
                    android:layout_marginStart="@dimen/dp_5"
                    android:textSize="@dimen/sp_12"
                    android:textColor="#999999"
                    tools:text="某某店铺"
                    tools:ignore="SmallSp"
                    android:visibility="gone"
                    />

            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_alignParentBottom="true"
                    >
                    <LinearLayout
                        android:id="@+id/layout_01"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_above="@+id/layout_02"
                        >

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="@dimen/dp_5"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:id="@+id/layout_03"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/quanhou11"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="¥"
                                    android:textColor="#000000"
                                    android:textSize="@dimen/sp_13"
                                    tools:ignore="HardcodedText" />


                                <TextView
                                    android:id="@+id/adapter_new_one_fragment_price"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="#000000"
                                    android:textSize="@dimen/sp_18"
                                    tools:text="29.0" />

                            </LinearLayout>


                        </RelativeLayout>

                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/layout_02"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_alignParentBottom="true"
                        tools:ignore="RtlHardcoded">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="bottom">
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <LinearLayout
                                    android:id="@+id/adapter_new_one_fragment_discount_layout"
                                    android:layout_width="@dimen/dp_57"
                                    android:layout_height="@dimen/dp_17"
                                    android:background="@mipmap/coponbg"
                                    android:layout_gravity="center"
                                    android:orientation="horizontal">
                                    <TextView
                                        android:id="@+id/adapter_new_one_fragment_discount"
                                        android:layout_width="wrap_content"
                                        android:layout_height="@dimen/dp_17"
                                        android:minWidth="@dimen/dp_40"
                                        android:gravity="center"
                                        android:layout_marginStart="@dimen/dp_17"
                                        android:paddingEnd="@dimen/dp_6"
                                        android:paddingStart="@dimen/dp_5"
                                        android:textSize="@dimen/sp_10"
                                        android:textColor="#FFFFFF"
                                        tools:text="100元"/>

                                </LinearLayout>
                                <LinearLayout
                                    android:id="@+id/adapter_new_one_fragment_estimate"
                                    android:layout_width="@dimen/dp_57"
                                    android:layout_height="@dimen/dp_17"
                                    android:layout_marginStart="@dimen/dp_5"
                                    android:background="@drawable/bg_rect_strock_black_2dp"
                                    android:orientation="horizontal"
                                    android:gravity="center"
                                    >

                                    <TextView
                                        android:id="@+id/adapter_new_one_fragment_estimate_text"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="#000000"
                                        android:textSize="@dimen/sp_10"
                                        tools:text="奖 2元" />

                                </LinearLayout>
                            </LinearLayout>
                        </RelativeLayout>


                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>

        </LinearLayout>
    </LinearLayout>

</LinearLayout>