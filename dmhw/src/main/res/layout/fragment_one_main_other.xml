<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/fragment_one_other_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="none"
            />
     <View
         android:id="@+id/fragment_one_other_line"
         android:layout_width="match_parent"
         android:layout_height="@dimen/dp_1"
         android:visibility="invisible"
         />
    <LinearLayout
        android:id="@+id/fragment_one_other_header"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_35"
        android:orientation="vertical"
        android:background="@color/white"
        android:visibility="gone"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/fragment_one_other_one"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_weight="1"
                tools:ignore="NestedWeights"
                >

                <TextView
                    android:id="@+id/fragment_one_other_one_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/mainColor"
                    android:text="综合"
                    tools:ignore="HardcodedText" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/fragment_one_other_two"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/fragment_one_other_two_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/sp_14"
                    android:textColor="#666666"
                    android:text="销量"
                    tools:ignore="HardcodedText" />

                <ImageView
                    android:id="@+id/fragment_one_other_two_image"
                    android:layout_width="@dimen/dp_7"
                    android:layout_height="@dimen/dp_11"
                    android:layout_marginStart="@dimen/dp_5"
                    android:scaleType="fitXY"
                    android:src="@mipmap/fragment_team_default"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/fragment_one_other_three"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/fragment_one_other_three_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/sp_14"
                    android:textColor="#666666"
                    android:text="佣金"
                    tools:ignore="HardcodedText" />

                <ImageView
                    android:id="@+id/fragment_one_other_three_image"
                    android:layout_width="@dimen/dp_7"
                    android:layout_height="@dimen/dp_11"
                    android:layout_marginStart="@dimen/dp_5"
                    android:scaleType="fitXY"
                    android:src="@mipmap/fragment_team_default"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/fragment_one_other_four"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/fragment_one_other_four_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/sp_14"
                    android:textColor="#666666"
                    android:text="价格"
                    tools:ignore="HardcodedText" />

                <ImageView
                    android:id="@+id/fragment_one_other_four_image"
                    android:layout_width="@dimen/dp_7"
                    android:layout_height="@dimen/dp_11"
                    android:layout_marginStart="@dimen/dp_5"
                    android:scaleType="fitXY"
                    android:src="@mipmap/fragment_team_default"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_weight="1"
                android:gravity="center_vertical">

                <View
                    android:layout_width="@dimen/dp_1"
                    android:layout_height="@dimen/dp_12"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:background="#eaeaea"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:gravity="center">

                    <CheckBox
                        android:id="@+id/fragment_one_other_screen_check"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:button="@null"
                        android:gravity="center"
                        android:text="筛选"
                        android:textSize="@dimen/sp_14"
                        android:textColor="#666666"
                        android:drawablePadding="@dimen/dp_7"
                        tools:ignore="HardcodedText" />

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="#eaeaea"/>

        <View
            android:id="@+id/fragment_one_new_mask"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone"
            android:background="@color/white"/>

    </LinearLayout>

    <ImageView
        android:id="@+id/return_top"
        android:layout_width="@dimen/dp_35"
        android:layout_height="@dimen/dp_35"
        android:scaleType="centerCrop"
        android:src="@mipmap/return_top"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="@dimen/dp_18"
        android:layout_marginEnd="@dimen/dp_20"/>
    <ImageView
        android:id="@+id/fragment_one_mask"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"/>
    <include
        android:id="@+id/network_mask"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        layout="@layout/view_no_net_empty"
        android:visibility="gone"
        />

</RelativeLayout>